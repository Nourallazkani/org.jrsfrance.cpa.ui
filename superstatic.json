{
    "rewrites": [
        {
            "source": "/**",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "**/*",
            "headers": [
                {
                    "key": "Cache-control",
                    "value": "max-age=-1"
                }
            ]
        }
    ]
}